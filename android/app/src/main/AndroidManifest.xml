        android:authorities="com.tolulopeahmed.MyFundApp.expo.modules.filesystem.FileSystemFileProvider"
        android:exported="false"
        android:grantUriPermissions="true"
        tools:replace="android:authorities">
        <meta-data
          android:name="android.support.FILE_PROVIDER_PATHS"
          android:resource="@xml/expo_fileprovider_paths"
          tools:replace="android:resource"/>

    </provider>
  
  </application>
</manifest>